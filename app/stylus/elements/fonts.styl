@charset "UTF-8";

@charset "UTF-8";
    @font-face
      font-family "ironwill-font"
      src url("fonts/ironwill-font.eot")
      src url("fonts/ironwill-font.eot?#iefix") format("embedded-opentype"),
    url("fonts/ironwill-font.woff") format("woff"),
    url("fonts/ironwill-font.ttf") format("truetype"),
    url("fonts/ironwill-font.svg#ironwill-font") format("svg")
      font-weight normal
      font-style normal

[data-icon]:before
  font-family "ironwill-font" !important
  content attr(data-icon)
  font-style normal !important
  font-weight normal !important
  font-variant normal !important
  text-transform none !important
  speak none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

[class^="icon-"]:before,
[class*=" icon-"]:before
  font-family "ironwill-font" !important
  font-style normal !important
  font-weight normal !important
  font-variant normal !important
  text-transform none !important
  speak none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-menu
  &:before
    content "\61"

.icon-twitter
  &:before
    content "\62"

.icon-instagram
  &:before
    content "\63"

.icon-facebook
  &:before
    content "\64"

.icon-search
  &:before
    content "\66"

.icon-angle-down
  &:before
    content "\65"


